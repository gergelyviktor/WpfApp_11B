<Window x:Class="WpfApp2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="123" Margin="221,255,0,0" VerticalAlignment="Top" Width="118"/>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="91" Margin="29,287,0,0" VerticalAlignment="Top" Width="143"/>
        <Label x:Name="label1" Content="Label" HorizontalAlignment="Left" Margin="52,48,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textbox1" HorizontalAlignment="Left" Margin="52,109,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button1" Content="Számol" HorizontalAlignment="Left" Margin="52,168,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <CheckBox x:Name="checkbox1" Content="CheckBox" HorizontalAlignment="Left" Margin="52,240,0,0" VerticalAlignment="Top"/>
        <RadioButton Content="RadioButton" HorizontalAlignment="Left" Margin="52,302,0,0" VerticalAlignment="Top" GroupName="neme"/>
        <RadioButton Content="RadioButton" HorizontalAlignment="Left" Margin="52,342,0,0" VerticalAlignment="Top" GroupName="neme"/>
        <ListBox x:Name="listbox1" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="221,18,442,216" SelectionChanged="ListBox_SelectionChanged"/>
        <ComboBox x:Name="combobox1" HorizontalAlignment="Left" Margin="400,19,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="combobox1_SelectionChanged"/>
        <Image x:Name="image1" HorizontalAlignment="Left" Height="100" Margin="228,264,0,0" VerticalAlignment="Top" Width="100"/>
        <DataGrid x:Name="datagrid1" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="400,91,136,95"/>

    </Grid>
</Window>
